<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>User</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <tr>
    <td><%= project.name %></td>
    <td><%= project.description %></td>
    <td><%= project.user_id %></td>
    <td><%= link_to "Show", project%></td>
    <td><%= link_to "Edit", edit_project_path(project)%></td>
    <td><%= button_to "Delete", project, form: { data: { turbo_confirm: "Are you sure?" } }, method: :delete %></td>
    </td>
  </tr>
  </tbody>
</table>